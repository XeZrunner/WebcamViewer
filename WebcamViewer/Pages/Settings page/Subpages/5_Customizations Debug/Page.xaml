<UserControl x:Class="WebcamViewer.Pages.Settings_page.Subpages._5_Customizations_Debug.Page"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WebcamViewer.Pages.Settings_page.Subpages._5_Customizations_Debug"
             mc:Ignorable="d" 
             d:DesignWidth="500" d:DesignHeight="572" Background="{DynamicResource settingsPage_background}">
    <Grid>

        <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled" Visibility="Visible">
            <StackPanel Margin="10">
                <StackPanel.Resources>
                    <Style TargetType="TextBox">
                        <Setter Property="Foreground" Value="{DynamicResource settingsPage_foregroundText}"/>
                    </Style>
                </StackPanel.Resources>

                <TextBlock x:Name="settingsPage_DefaultCustomiazionsDebugPage_MainTextBlock" FontFamily="Consolas">DEFAULT CUSTOMIZATIONS FILE CONTENTS GO HERE</TextBlock>

            </StackPanel>
        </ScrollViewer>

    </Grid>
</UserControl>
